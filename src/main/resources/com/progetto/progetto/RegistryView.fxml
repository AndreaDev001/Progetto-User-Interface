<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<VBox alignment="TOP_CENTER" minHeight="-Infinity" minWidth="500.0" spacing="10.0" styleClass="background" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.progetto.progetto.controller.RegisterController">
   <children>
      <VBox spacing="14.0">
         <children>
            <VBox>
               <children>
                  <Label text="Enter Your Email" />
                  <TextField fx:id="emailField" />
                  <Label fx:id="emailError" text="Email Invalida o Usata" />
               </children>
            </VBox>
            <VBox>
               <children>
                  <Label text="Enter a password" />
                  <TextField fx:id="passwordField" />
                  <Label fx:id="passwordError" text="Password Invalida" />
               </children>
            </VBox>
         </children>
      </VBox>
      <Button fx:id="confirmButton" disable="true" mnemonicParsing="false" onAction="#onConfirmButton" text="Confirm" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
